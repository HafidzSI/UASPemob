<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.isumatsumi.barangapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.isumatsumi.barangapp.activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.isumatsumi.barangapp.activity.RegisterActivity" />
        <activity android:name="com.example.isumatsumi.barangapp.activity.MainActivity" />
        <activity android:name="com.example.isumatsumi.barangapp.activity.BarangActivity" />
        <activity android:name="com.example.isumatsumi.barangapp.activity.BarangDetailActivity" />
        <activity android:name="com.example.isumatsumi.barangapp.activity.TambahBarangActivity"></activity>
        <activity android:name="com.example.isumatsumi.barangapp.activity.FavoriteActivity"></activity>
    </application>

</manifest>